<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    tools:context=".HomeActivity">


    <ImageView
        android:id="@+id/image1"
        app:srcCompat="@drawable/build7"
        android:layout_width="195dp"
        android:layout_height="180dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="0dp"
        android:layout_marginTop="0dp"/>
        <ImageView
            android:id="@+id/image3"
            app:srcCompat="@drawable/fornt7"
            android:layout_width="180dp"
            android:layout_height="180dp"
            android:layout_alignParentStart="true"
            android:layout_alignParentTop="true"
            android:layout_marginStart="195dp"
            android:layout_marginTop="0dp"/>
    <TextView
        android:layout_width="match_parent"
        android:layout_height="22dp"
        android:textSize="15sp"
        android:text="Building7"
        android:gravity="center"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="0dp"
        android:layout_marginTop="180dp"/>
    <TextView
        android:id="@+id/available1"
        android:layout_width="120dp"
        android:layout_height="22dp"
        android:textSize="15sp"
        android:text="Available"
        android:gravity="center"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="100dp"
        android:layout_marginTop="203dp"/>
    <TextView
        android:id="@+id/values1"
        android:layout_width="40dp"
        android:layout_height="22dp"
        android:textSize="15sp"
        android:text="35"
        android:gravity="center"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="220dp"
        android:layout_marginTop="203dp"/>

    <ImageView
        android:id="@+id/image2"
        android:layout_width="440dp"
        android:layout_height="200dp"
        app:srcCompat="@drawable/build6"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="0dp"
        android:layout_marginTop="225dp"/>
    <TextView
        android:layout_width="match_parent"
        android:layout_height="22dp"
        android:textSize="15sp"
        android:text="Building6"
        android:gravity="center"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="0dp"
        android:layout_marginTop="425dp"/>
    <TextView
        android:id="@+id/available2"
        android:layout_width="120dp"
        android:layout_height="22dp"
        android:textSize="15sp"
        android:text="Available"
        android:gravity="center"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="100dp"
        android:layout_marginTop="445dp"/>
    <TextView
        android:id="@+id/values2"
        android:layout_width="40dp"
        android:layout_height="22dp"
        android:textSize="15sp"
        android:text="47"
        android:gravity="center"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="220dp"
        android:layout_marginTop="445dp"/>


</RelativeLayout>
